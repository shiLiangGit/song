/**app.wxss**/
.container{ width: 100%; height: 100%; background: #f1f1f1; position: absolute; top: 0; left: 0;} 
.fl{ float: left;}
.fr{ float: right;}
.clear::after{ content: ""; display: block; height: 0; clear: both;}
button::after{ content: ""; border: none;}

/**底部样式**/
.footer{ position: fixed; width: 100%; height: 124rpx; background: #fff; overflow: hidden; bottom: 0; left: 0;}
.footer button,.footer navigator{background: #fff; float: left; width: 33.33%; text-align: center; line-height:1.5;font-size: 30rpx; color: #888888;}
.footer button image,.footer navigator image{ width: 60rpx; height: 60rpx; display: block; margin: 14.5rpx auto 3rpx auto;}