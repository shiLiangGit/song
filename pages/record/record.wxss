.page_nav{ width: 100%; height: 90rpx; line-height: 90rpx; background: #fff; border-bottom: 5rpx solid #fff;}
.page_nav view{ float: left; width: 18%; margin:0 16%; text-align: center; font-size: 32rpx; color: #a7a7a7;}
.page_nav view.select{ border-bottom: 5rpx solid #d7563c; color: #d7563c;}
.page_je{ padding-top: 30rpx;}
.user_head{ width: 122rpx; height: 122rpx; border-radius: 50%; overflow: hidden; margin: 0 auto;}
.user_head image{ width: 100%; height: 100%;}
.user_name{ text-align: center; font-size: 32rpx; margin: 30rpx 0 60rpx;}
.user_name text{ color: #5b6f96; margin-right: 3%;}
.page_jelist>view{ float: left; width: 50%; text-align: center;}
.page_jeitem1 view,.page_jeitem2 view{ font-size: 32rpx; color: #a9a9a9; margin-bottom: 10rpx;}
.page_jeitem1 view text,.page_jeitem2 view text{ font-size: 33rpx; color: #333333;}
.page_userlist{ margin-top: 80rpx; background: #fff; height: 500rpx; overflow-y: auto;-webkit-overflow-scrolling:touch;}
.page_useritem,.page_setting{ margin: 0 27rpx;}
.page_useritem .page_useritemtop text,.page_useritem .page_useritemtop view text,.page_useritembottom{ font-size: 32rpx; color: #1f1f1f;}
.page_useritem .page_useritemtop view{ font-size: 32rpx;}
.page_useritem .page_useritemtop view text{ margin-right: 10rpx; font-size: 32rpx;}
.page_useritem .page_useritemtop>text{ width: 500rpx; height: 40rpx; overflow: hidden;}
.page_useritembottom{color:#5b5b5b; padding: 16rpx 0 19rpx;}
.page_useritemtop{ padding-top: 5px;}
.page_useritem{ border-bottom: 2rpx solid #e8e8e8;}
.page_setting{ height: 75rpx; line-height: 75rpx;}
.page_setting button{ font-size: 32rpx; color: #7a7a7a; background: none;}

.common_problem{ text-align: center; font-size: 30rpx; color: #435a87;}
.common_problempop{ position: fixed; left: 0; bottom: 0; width: 100%; z-index: 99; background: #e8e8e8; height: 55%; display: none;}
.common_problempop navigator{ width: 100%; height: 99rpx; line-height: 99rpx; display: block; text-align: center; background: #f5f5f5; border-bottom: 1px solid #e8e8e8; font-size: 34rpx; color: #030303;}
.common_problempop navigator:last-child{ margin-top: 12rpx;}
.common_problembar{ position: absolute; bottom: 0; left: 0; width: 100%;}

.page_txzc{ position: fixed; top: 0; left: 0; z-index: 100; background: rgba(0,0,0,0.5); width: 100%; height: 100%;}
.page_txzcbar{ width: 560rpx; height: 500rpx; background: #fff; border-radius: 5px; position: absolute; top:0;left: 0;right: 0;bottom: 0;margin: auto}
.page_txztl{ text-align: center; height: 94rpx; line-height: 94rpx; font-size: 34rpx; border-bottom: 1px solid #a9deab;}



.pagezc_input{ width: 500rpx; border-bottom: 1px solid #eaeaea; padding: 40rpx 0 15rpx 0; margin: 0 auto;}
.pagezc_input text{ font-size: 32rpx; color: #333; width: 130rpx; text-align: right; display: inline-block;}
.pagezc_input input{ width: 370rpx; float: right; font-size: 32rpx; height: 45rpx; line-height: 45rpx;}
.pagezc_btn{ width: 500rpx; height: 80rpx; line-height: 80rpx; background: #1aad19; color: #fff; font-size: 34rpx; margin-top: 40rpx;}
.pagezc_ts{ font-size: 28rpx; color: #333; display: block; text-align: center; height: 64rpx; line-height: 64rpx;}
.close{ position: absolute; top: 13rpx; left: 40rpx; color: #ababab; font-size: 44rpx;}
.wxts_txt{ width: 500rpx; font-size: 32rpx; color: #6d716d; margin: 75rpx auto;}

